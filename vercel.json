{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/blog/(.*)", "destination": "/blog/$1" },
    { "source": "/api/(.*)", "destination": "/api/$1" }
  ],
  "routes": [
    {
      "src": "/api/diagnose",
      "dest": "/api/diagnose.js"
    }
  ]
}
