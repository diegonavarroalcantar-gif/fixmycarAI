{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" },
    { "src": "blog/**/*", "use": "@vercel/static" },
    { "src": "api/diagnose.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/diagnose", "dest": "/api/diagnose.js" },
    { "src": "/blog/(.*)", "dest": "/blog/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
