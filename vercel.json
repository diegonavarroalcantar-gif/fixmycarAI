{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "api/diagnose.js", "use": "@vercel/node" },
    { "src": "blog/**/*", "use": "@vercel/static" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/blog/(.*)", "dest": "/blog/$1" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}
